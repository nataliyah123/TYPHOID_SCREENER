<FirstPage>:
    name: "first_page"
    #MDScreen:
        #MDBoxLayout:
        #MDCard:
            #size_hint: None, None
            #size: 300, 450
            #elevation:10
            #padding: 65
            #spacing: 25
            #pos_hint: {"center_x": 0.5, "center_y": 0.5}
    orientation: "vertical"

    MDLabel:
        text: "Disclaimer"
        font_style: 'H5'
        pos_hint:{'center_x':0.8,'center_y':0.8}   
        #valign: 'top'     
        #pos_hint: {"center_x": 0.5, "center_y": 0.5}
        #elevation: 10
