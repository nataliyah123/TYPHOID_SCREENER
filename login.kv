<SecondPage>:
    name: "second_page"    

        # Show login page #2
    MDCard:    
        size_hint: None, None
        size: 300, 450
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        elevation: 10
        padding: 65
        spacing: 25
        orientation: 'vertical'
        
        MDTextFieldRound:
            id: user
            icon_left: "account-check"
            hint_text: "Name"
            foreground_color: 1, 0, 1, 1
            size_hint_x: None
            width: 220
            font_size: 20
            pos_hint: {"center_x": 0.5}
        MDTextFieldRound:
            id: password
            icon_left: "key-variant"
            hint_text: "Password"
            foreground_color: 1, 0, 1, 1
            size_hint_x: None
            width: 220
            font_size: 20            
            pos_hint: {"center_x": 0.5}
            password: True
        MDRectangleFlatButton:
            text: "LOG IN"
            font_size: 15
            theme_text_color: "Custom"
            text_color: 0, 0, 1, 1
            pos_hint: {"center_x": 0.5}
            on_press: app.Login()
        MDRectangleFlatButton:
            text: "SIGN UP"
            font_size: 15
            theme_text_color: "Custom"
            text_color: 0, 0, 1, 1
            pos_hint: {"center_x": 0.5}
            on_press: root.manager.current = "third_page"   
     

<ThirdPage>:
    name: "third_page"  
    # Show doctor and tech sign up page #3
    MDCard:
        size_hint: None, None
        size: 300, 450
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        elevation: 10
        padding: 55
        spacing: 35
        orientation: 'vertical'
        MDIcon:
            icon: 'account'
            icon_color: 0, 0, 0, 0
            halign: 'center'
            font_size: 180
            
        ScrollView:
            #size_hint_y: 1.93
            pos_hint: {'x':0, 'y': .11}
            do_scroll_x: False
            do_scroll_y: True
            GridLayout:
                size:(root.width, 800)
                size_hint_x: None
                size_hint_y: None
                cols: 1
                height: self.minimum_height
                row_default_height: 60
                row_force_default: True
                #username
                MDTextFieldRound:
                    id: user_signup
                    icon_left: "account-check"
                    hint_text: "Username"
                    foreground_color: 1, 0, 1, 1
                    size_hint_x: None
                    width: 220
                    font_size: 20
                    pos_hint: {"center_x": 0.5}
                #email
                MDTextFieldRound:
                    id: signup_email
                    icon_left: "email"
                    hint_text: "Email"
                    foreground_color: 1, 0, 1, 1
                    size_hint_x: None
                    width: 220
                    font_size: 20
                    pos_hint: {"center_x": 0.5}    
                #occupation
                MDTextFieldRound:
                    id: occupation
                    icon_left: "doctor"
                    hint_text: "Doctor / technician"
                    foreground_color: 1, 0, 1, 1
                    size_hint_x: None
                    width: 220
                    font_size: 20
                    pos_hint: {"center_x": 0.5} 
                #organization
                MDTextFieldRound:
                    id: organization
                    icon_left: "progress-wrench"
                    hint_text: "organization"
                    foreground_color: 1, 0, 1, 1
                    size_hint_x: None
                    width: 220
                    font_size: 20
                    pos_hint: {"center_x": 0.5}                       
                                
                #password
                MDTextFieldRound:
                    id: password
                    icon_left: "key-variant"
                    hint_text: "Password"
                    foreground_color: 1, 0, 1, 1
                    size_hint_x: None
                    width: 220
                    font_size: 20            
                    pos_hint: {"center_x": 0.5}        
                    password: True
                
                MDRectangleFlatButton:
                    text: "SIGN UP"
                    font_size: 15
                    theme_text_color: "Custom"
                    text_color: 0, 0, 1, 1
                    pos_hint: {"center_x": 0.5}
                    on_press: app.Sign_Up_doctech()                 

<FourthPage>:
